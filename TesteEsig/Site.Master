                        <Items>
                            <asp:MenuItem Text="Usuário" Value="Usuário"></asp:MenuItem>
                            <asp:MenuItem Text="Funcionários" Value="Funcionários"></asp:MenuItem>
                            <asp:MenuItem Text="Cargos" Value="Cargos"></asp:MenuItem>
                            <asp:MenuItem Text="Sair" Value="Sair"></asp:MenuItem>
                        </Items>
                        <StaticHoverStyle BackColor="#284E98" ForeColor="White" />
                        <StaticMenuItemStyle HorizontalPadding="5px" VerticalPadding="2px" />
                        <StaticSelectedStyle BackColor="#507CD1" />
                    </asp:Menu>
                </asp:ContentPlaceHolder>
            </div>
        </main>
        <footer>
            site desenvolvido por Ana Luisa Estevam
        </footer>
    </form>
